
{
  "version": 2,
  "routes": [
    {
      "src": "/how-it-works/.*",
      "dest": "/index.html"
    },
    {
      "src": "/recipe/.*",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
